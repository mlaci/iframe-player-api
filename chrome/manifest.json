{
    "manifest_version": 2,
    "name": "Iframe Player API",
    "version": "0.1.0",
    "icons": {
        "16":"icons/logo16.png",
        "48":"icons/logo48.png",
        "128":"icons/logo128.png"
    },
    "page_action": {
        "default_icon": "icons/logo32.png",
        "default_title": "Iframe Player API"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": [
        "webNavigation",
        "activeTab",
        "declarativeContent"
    ],
    "optional_permissions": ["<all_urls>"],
    "web_accessible_resources": ["content-script.js"]
}